{
  "author": "BWsix",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/lib.esm.js",
  "name": "@VFLC/explode.js",
  "typings": "dist/index.d.ts",
  "version": "0.1.0",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "size-limit": [
    {
      "limit": "10 KB",
      "path": "dist/lib.cjs.production.min.js"
    },
    {
      "limit": "10 KB",
      "path": "dist/lib.esm.js"
    }
  ]
}
